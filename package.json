{
  "name": "diffscribe",
  "version": "1.0.2",
  "description": "AI-powered commit message generator for Conventional Commits",
  "type": "module",
  "bin": {
    "dcs": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc && shx chmod +x dist/index.js",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.0",
    "@openrouter/sdk": "^0.5.1",
    "chalk": "^5.4.1",
    "clipboardy": "^4.0.0",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "execa": "^9.5.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "shx": "^0.3.4",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "git",
    "commit",
    "conventional-commits",
    "cli",
    "ai",
    "diffscribe",
    "dcs"
  ],
  "preferGlobal": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raghavvvgaba/diffscribe.git"
  },
  "author": "Raghav Gaba <raghav@example.com>",
  "bugs": {
    "url": "https://github.com/raghavvvgaba/diffscribe/issues"
  },
  "homepage": "https://github.com/raghavvvgaba/diffscribe#readme",
  "license": "MIT"
}
